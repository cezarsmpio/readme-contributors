{
  "name": "readme-contributors",
  "main": "app/server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "prettier": "prettier ./**/*.js",
    "lint": "eslint ./",
    "validate": "npm run lint && npm run prettier",
    "format": "npm run prettier -- --write"
  },
  "author": "Cezar Sampaio",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "chrome-aws-lambda": "^1.18.1",
    "dotenv": "^8.0.0",
    "fogo": "^1.0.0",
    "lodash.range": "^3.2.0",
    "lodash.template": "^4.5.0",
    "node-cache": "^4.2.0",
    "puppeteer-core": "^1.18.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.--write": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
